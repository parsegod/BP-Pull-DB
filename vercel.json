{
  "rewrites": [
    {
      "source": "/HAH",
      "destination": "/helpers/helpers.html"
    },

    {
      "source": "/blocked",
      "destination": "/blocked.html"
    },

    {
      "source": "/home",
      "destination": "/index.html",
      "has": [
        {
          "type": "cookie",
          "key": "blubase_verified",
          "value": "true"
        }
      ]
    },

    {
      "source": "/verify",
      "destination": "/verify.html"
    },


    {
      "source": "/(.*)",
      "destination": "/index.html",
      "has": [
        {
          "type": "cookie",
          "key": "blubase_verified",
          "value": "true"
        }
      ]
    },
  
    {
      "source": "/(.*)",
      "destination": "/verify.html"
    }
  ]
}
