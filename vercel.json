{
  "redirects": [
    { 
      "source": "/:path*", 
      "destination": "/moved", 
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "(www\\.)?parsed\\.top"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/moved", "destination": "/moved.html" }
  ]
}
